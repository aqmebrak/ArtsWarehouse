{
	"name": "no1yz.art",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:start": "docker-compose up --detach",
		"db:push": "drizzle-kit push",
		"db:push-prod": "dotenv -e .env.prod drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:migrate-prod": "dotenv -e .env.prod drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:studio-prod": "dotenv -e .env.prod drizzle-kit studio",
		"db:seed": "npx tsx src/lib/server/db/seed.ts",
		"db:verify": "npx tsx src/lib/server/db/verify.ts",
		"db:scrape": "npx tsx src/lib/server/db/scraper.ts",
		"db:reset": "npm run db:push && npm run db:seed"
	},
	"devDependencies": {
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.548.0",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.6",
		"@playwright/test": "^1.54.2",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-vercel": "^6.0.0",
		"@sveltejs/kit": "^2.43.5",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.17.2",
		"@types/p5": "^1.7.6",
		"@typescript-eslint/eslint-plugin": "^8.15.0",
		"@typescript-eslint/parser": "^8.15.0",
		"@vercel/postgres": "^0.10.0",
		"autoprefixer": "10.4.21",
		"axios": "^1.12.2",
		"bits-ui": "^2.11.5",
		"cheerio": "^1.1.2",
		"clsx": "^2.1.1",
		"dotenv-cli": "^10.0.0",
		"drizzle-kit": "^0.28.1",
		"drizzle-seed": "^0.3.0",
		"eslint": "9.34.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.12.5",
		"husky": "9.1.7",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.0",
		"svelte": "5.42.2",
		"svelte-check": "4.3.3",
		"sveltekit-superforms": "^2.22.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.4.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.46.2",
		"vite": "7.1.12",
		"vitest": "4.0.18",
		"zod": "^4.0.0"
	},
	"dependencies": {
		"@vercel/analytics": "1.5.0",
		"@vercel/speed-insights": "1.2.0",
		"dotenv": "^17.0.0",
		"drizzle-orm": "^0.36.4",
		"drizzle-zod": "^0.8.0",
		"lucide-svelte": "^0.548.0",
		"p5": "1.11.11",
		"phaser": "^3.88.2",
		"phaser3-rex-plugins": "^1.80.15",
		"postgres": "^3.4.4",
		"runed": "^0.35.0"
	}
}
